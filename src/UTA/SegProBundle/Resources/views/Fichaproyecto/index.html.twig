{% extends 'UTASegProBundle::admin.html.twig' %}


    {% block header %}
        <h1>Fichaproyecto list</h1>
    {% endblock %}
            
    {% block content %}
    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Codigoidentificacion</th>
                <th>Nombrecorto</th>
                <th>Nombre</th>
                <th>Fechainicio</th>
                <th>Fechatermino</th>
                <th>Fechapostulacion</th>
                <th>Fechaadjudicacion</th>
                <th>Montoadjudicado</th>
                <th>Acciones</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('admin_fichaproyecto_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.codigoidentificacion }}</td>
                <td>{{ entity.nombrecorto }}</td>
                <td>{{ entity.nombre }}</td>
                <td>{% if entity.fechainicio %}{{ entity.fechainicio|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechatermino %}{{ entity.fechatermino|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechapostulacion %}{{ entity.fechapostulacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{% if entity.fechaadjudicacion %}{{ entity.fechaadjudicacion|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.montoadjudicado }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('admin_fichaproyecto_show', { 'id': entity.id }) }}">Mostrar</a>
                        </li>
                        <li>
                            <a href="{{ path('admin_fichaproyecto_edit', { 'id': entity.id }) }}">Editar</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_fichaproyecto_new') }}">
                Agregar nuevo registro
            </a>
        </li>
    </ul>
    {% endblock %}
