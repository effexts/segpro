{% extends 'UTASegProBundle::admin.html.twig' %}
{% block title %} Fuentes de Financiamiento {% endblock %}


    {% block header %}
        <h1>Fuentes de Financiamiento</h1>
    {% endblock %}
            
{% block content %}
    <table class="record_properties">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Descripci√≥n</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Fecha Apertura</th>
                <td>{{ entity.fechaapertura|date("m/d/Y") }}</td>
            </tr>
            <tr>
                <th>Fecha Cierre</th>
                <td>{{ entity.fechacierre|date('d-m-Y') }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('admin_fuentedefinanciamiento') }}">
                Volver a la lista
            </a>
        </li>
        <li>
            <a href="{{ path('admin_fuentedefinanciamiento_edit', { 'id': entity.id }) }}">
                Editar
            </a>
        </li>
        <li>
            <form action="{{ path('admin_fuentedefinanciamiento_delete', { 'id': entity.id }) }}" method="post">
                <input type="hidden" name="_method" value="DELETE" />
                {{ form_widget(delete_form) }}
                <button type="submit">Eliminar</button>
            </form>
        </li>
    </ul>
{% endblock %}
